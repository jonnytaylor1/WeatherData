<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DetailedTableController">
   <children>
         <TableView fx:id="tableView" prefHeight="267.0" prefWidth="900.0" stylesheets="@../../JavaFX%20Coursework/sample/stylesheets/tablestyles.css">
           <columns>
               <TableColumn fx:id="colYear" prefWidth="120.0" text="Year" />
               <TableColumn fx:id="colHighestTMax" minWidth="0.0" prefWidth="120.0" text="Highest TMax" />
               <TableColumn fx:id="colLowestTMin" prefWidth="120.0" text="Lowest TMin" />
               <TableColumn fx:id="colTotalAfDays" minWidth="0.0" prefWidth="120.0" text="Total AF Days" />
               <TableColumn fx:id="colTotalRainfall" prefWidth="120.0" text="Total Rainfall" />
           </columns>
         </TableView>
      <AnchorPane prefHeight="200.0" prefWidth="900.0">
         <children>
          <fx:include source="graphs.fxml" />
         </children>
      </AnchorPane>
   </children>
</VBox>
